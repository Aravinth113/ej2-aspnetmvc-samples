@using Syncfusion.EJ2
@section ControlsSection{
    <div class="control-section">
        <!-- sample level element  -->
        <div class="col-lg-12 col-sm-12 col-md-12 center">
            Click the button to view the sample
        </div>
        <div class="col-lg-12 col-sm-12 col-md-12 center">
            @Html.ActionLink("Open in New Tab", "BulletAndNumber", "DocumentEditor", null, new { @class = "e-btn", @target = "_blank" })
        </div>
    </div>
}
@section ActionDescription{
    <div id="action-description">
         <p>This example demonstrates bullets and numbering support in document editor. Both single list and multi-level lists are supported. </p>
    </div>
}
@section Description{
    <div id="description">
       <p>In this example, you can use, add, or modify the list formatting in document editor.</p>
    
    <p style="display: block"> More information about the document editor features can be found in this <a target="_blank" href="https://ej2.syncfusion.com/aspnetmvc/documentation/document-editor/getting-started.html">documentation section.</a>
    </p>
    </div>
}

@section Scripts{
    <script></script>
}

@section Meta{
    <meta name="description" content="The Document Editor supports bullets and numbering. Both single list and multi-level lists can be added." />
}

<style>
    .center {
        text-align: center;
        font-size: 13px;
        font-weight: 400;
        margin-top: 20px;
    }
</style>